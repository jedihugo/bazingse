{
  "name": "bazingse-app",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "api": "cd ../api && python3 run_bazingse.py",
    "api:install": "cd ../api && pip3 install -r requirements.txt",
    "cap:sync": "npm run build && npx cap sync",
    "cap:ios": "npm run build && npx cap sync ios && npx cap open ios",
    "cap:run:ios": "npm run build && npx cap sync ios && npx cap run ios"
  },
  "dependencies": {
    "@capacitor/cli": "^8.0.1",
    "@capacitor/core": "^8.0.1",
    "@capacitor/ios": "^8.0.1",
    "@trpc/client": "^11.10.0",
    "@trpc/server": "^11.10.0",
    "superjson": "^2.2.6",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^20.0.0",
    "lunar-typescript": "^1.8.6",
    "playwright": "^1.57.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
