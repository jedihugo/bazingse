  "calculation_rules": {
    "day_officer": {
      "formula": "officer_index = (day_branch_index - month_branch_index + 12) % 12",
      "officers_in_order": ["jian", "chu", "man", "ping", "ding", "zhi", "po", "wei", "cheng", "shou", "kai", "bi"],
      "example_month_1": "Tiger month: Tiger=建, Rabbit=除, Dragon=滿, Snake=平, Horse=定, Goat=執, Monkey=破, Rooster=危, Dog=成, Pig=收, Rat=開, Ox=閉",
