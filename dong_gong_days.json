  "patterns_discovered": {
    "primary_pattern": {
      "name": "Branch Type is the PRIMARY Factor",
      "description": "The Day Branch type determines base tendency. Day Officer alone does NOT determine outcome.",
      "branch_types": {
        "fire_branches": {
          "branches": ["si (巳/Snake)", "wu (午/Horse)"],
          "tendency": "CAN be excellent with right officer + stars",
          "notes": "Pure Fire energy, receptive to Yellow Spiral stars"
        },
        "water_branches": {
          "branches": ["hai (亥/Pig)", "zi (子/Rat)"],
          "tendency": "CAN be good with right officer + stars",
          "notes": "Pure Water energy, receptive to auspicious stars"
        },
        "earth_storage_branches": {
          "chinese": "四库",
          "branches": ["chen (辰/Dragon)", "wei (未/Goat)", "xu (戌/Dog)", "chou (丑/Ox)"],
          "tendency": "ALMOST ALWAYS BAD regardless of officer",
          "notes": "Storage/Tomb branches trap energy, inherently problematic"
        },
        "wood_branches": {
          "branches": ["yin (寅/Tiger)", "mao (卯/Rabbit)"],
          "tendency": "Bad especially when same as month branch (建 Establish)",
          "notes": "Emptiness Day when branch = month"
        },
        "metal_branches": {
          "branches": ["shen (申/Monkey)", "you (酉/Rooster)"],
          "tendency": "Depends on season and Four Abandonment rule",
          "notes": "Metal pillars dire in Spring (四廢)"
        }
      }
    },

    "critical_evidence": {
      "same_officer_different_results": {
        "stable_officer": {
          "month_1_horse": { "branch_type": "Fire", "result": "ALL EXCELLENT/AUSPICIOUS" },
          "month_2_goat": { "branch_type": "Earth Storage", "result": "MOSTLY BAD (only Gui Wei auspicious)" }
        },
        "success_officer": {
          "month_1_dog": { "branch_type": "Earth Storage", "result": "ALL BAD" },
          "month_2_pig": { "branch_type": "Water", "result": "ALL EXCELLENT/AUSPICIOUS" }
        },
        "full_officer": {
          "month_1_dragon": { "branch_type": "Earth Storage", "result": "ALL BAD" },
          "month_2_snake": { "branch_type": "Fire", "result": "ALL AUSPICIOUS" }
        }
      },
      "conclusion": "Day Officer is NECESSARY but NOT SUFFICIENT. Branch Type determines if the officer can express positively."
    },

